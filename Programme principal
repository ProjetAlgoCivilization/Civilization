    nourriture, tour, nourritureTour, population, travailTour, tourAvCroissance: integer;
begin
      tour:=2;
      nourriture:=5;
      nourritureTour := 8;
      population := 14  ;
      travailTour := 3;
      tourAvCroissance:= 89;

      menu(nourriture, tour, nourritureTour, population, travailTour, tourAvCroissance);
      readln;

      readln;

end.
